  <script>
    const form = document.getElementById('checkoutForm')
    form.addEventListener('click', function (event) {
      event.preventDefault()
      console.log("<%= payment %>");
      paymentMode = '<%= payment %>'
      if (paymentMode == 'Razopay') {
        fetch('/genOrder', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },

        }).then(response => response.json())
          .then(data => {
            id = data.orderId
            
            let options = {
              "key": "rzp_test_PwI6HFeewvqmlD", // Enter the Key ID generated from the Dashboard
              "amount": '<%= totalPrice %>' + '00', // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
              "currency": "INR",
              "name": "H_loom", //your business name
              "description": "Test Transaction",
              "image": "",
              "order_id": id,
              "callback_url": "/product-checkout",
              "prefill": { //We recommend using the prefill parameter to auto-fill customer's contact information especially their phone number
                "name": "Akil", //your customer's name
                "email": "akil123@example.com",
                "contact": "9000090000" //Provide the customer's phone number for better conversion rates 
              },
              // "notes": {
              //   "address": address,
              //   "offer": offer
              // },
              "theme": {
                "color": "#455163"
              }
            };
            let rzp1 = new Razorpay(options);
            rzp1.open();
            
          })

      } else if (paymentMode == 'wallet') {
        const wallet = Number('<%= balance %>')
        const total = Number('<%=totalPrice%>')
        console.log(wallet, total);
        if (wallet - total < 0) {
          Swal.fire({
            title: "No Balance",
            text: "Not enough balance In Wallet",
            icon: "error",
            confirmButtonText: "OK"
          })
        } else {
          window.location.href = '/product-checkout'
        }
      } else {
        // cash one delivery
        // window.location.href = '/product-checkout'
        saveOrder();
      }
      
      function saveOrder() {
        $.ajax({
          url: "/product-checkout",
          method: "POST",
          

          success: function (response) {
            if (response.message == "1") {
              Swal.fire({
                title: "Success",
                text: "Order placed successfully",
                icon: "success",
                confirmButtonText: "OK",
              }).then((result) => {
                if (result.isConfirmed) {
                  window.location.href = "checkout-5"; // Redirect to profile page
                }
              });
            } else {
              Swal.fire({
                title: "Error",
                text: "Something went worng try again after sometimes",
                icon: "error",
                confirmButtonText: "OK",
              });
            }
          },
        });
      }

    })
  </script>




